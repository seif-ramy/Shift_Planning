{
  "name": "Shift_Planning",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/http-client": "^3.8.1",
    "@sap-cloud-sdk/resilience": "^3.8.1",
    "@sap/cds": "^7",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "ECEmploymentInformation": {
        "kind": "odata-v2",
        "model": "srv/external/ECEmploymentInformation",
        "credentials": {
          "destination": "SFSF",
          "path": "/odata/v2/EmpJob"
        }
      },
      "PLTUserManagement": {
        "kind": "odata-v2",
        "model": "srv/external/PLTUserManagement",
        "credentials": {
          "destination": "SFSF",
          "path": "/odata/v2/User"
        }
      }
    }
  }
}